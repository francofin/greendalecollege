									<li class='hide' id='hidden-id'>{{id}}</li>
									<li class='hide' id='hidden-id-{{id}}'>{{id}}</li>
									<li><button type="button" id="upvote-btn">like</button></li>
									<li><i class="mr-5 font-12 ion-ios-chatbubble-outline"></i>15</li>
									<li><i class="mr-5 font-12 ion-eye"></i>105</li>
